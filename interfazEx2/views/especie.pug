extends layout
block content
  .w3-card-4
    header.w3-container.w3-teal
      h1= nome
    .w3-container
      table.w3-table-all
        h2
            a(href="/") Voltar
        tr
          th Id
          th Freguesia
          th Estado
          th Número de intervenções;
        
        each l in list
          tr
            td
              a(href="/"+l.Id)= l.Id
            td= l.Freguesia
            td= l.Estado
            td= l['Número de intervenções']

              
    footer.w3-container.w3-teal
      h5 Generated by e10391
